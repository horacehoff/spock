func test(x) {
    print(x);
    return x+"!!!";
}

func main() {
let count = 0;
let limit = 1000000;
let result = 1;
while count < limit {
    result = result * 2;
    if result > 1000000 {
        result = result % 1000000;
    }
    count = count + 1;
}
print(result);
let ok = test("WOW");
print("OK IS "+ok);
}